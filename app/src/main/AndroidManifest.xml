<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeampierre.titanicshop" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.JanusShop" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.Categorias"
            android:label="@string/title_activity_categorias" >
        </activity>
        <activity
            android:name=".activities.Ordenes"
            android:label="@string/title_activity_ordenes" >
        </activity>
        <activity
            android:name=".activities.Facturas"
            android:label="@string/title_activity_facturas" >
        </activity>
        <activity
            android:name=".activities.DetailActivity"
            android:label="@string/title_activity_detail"
            android:parentActivityName=".activities.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jeampierre.titanicshop.activities.MainActivity" />
        </activity>
    </application>

</manifest>
